{
  "manifest_version": 2,
  "name": "M3u8 Test",
  "version": "1.0",
  
  "description": "Blah blah blah",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
	"default_title": "Click here!"
  },
  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
	  "js": ["contentscript.js"]
    }
  ],
  
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}